; PlatformIO Project Configuration File
[platformio]
src_dir = simulation
lib_dir = .
include_dir =

[env:witty]
platform = espressif8266
board = nodemcuv2
framework = arduino
build_flags =
    -DLED_R=D8
    -DLED_G=D6
    -DLED_B=D7
    -DLED_PIN=D5

[env:uno]
platform = atmelavr
board = uno
framework = arduino
